{
    "version": 2,
    "builds": [
        {
            "src": "src/api/index.ts",
            "use": "@vercel/node",
            "config": {
                "maxDuration": 60,
                "maxLambdaSize": "50mb",
                "runtime": "nodejs14.x"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/api/index.ts"
        }
    ]
}